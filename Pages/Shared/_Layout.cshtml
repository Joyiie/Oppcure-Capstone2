<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CapstoneR2</title>
    <link rel="stylesheet" href="~/bootsrap/bootstrap.css" asp-append-version="true" />
</head>
<body class="bg-mustard">
    <header>
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-sm-6 px-0">
                    <img src="~/images/logo1 (2).jpg" class="img-fluid" alt="Responsive image" />
                    
                    </div>
                </div>
            </div>
        
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted fixed-bottom">
        <div class="container">
            &copy; 2023 - Capstonep2 - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/jquery/jquery-3.6.3.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
    <script src="~/bootsrap/bootstrap.js"></script>
    <script src="~/scanner/html5-qrcode.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/qrcodejs/1.0.0/qrcode.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>